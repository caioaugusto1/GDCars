@model VendaDeAutomoveis.Entidades.Performance

<title>Cadastrar - Roda</title>

@{
    ViewBag.Title = "Cadastrar - Roda";
    Html.RenderPartial("_partial/_cabecalho_principal");
}

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <div class="main-container">
        <div class="dashboard-wrapper">
            <div class="row">
                <div class="col-md-12">
                    <div class="widget">
                        <div class="widget-header">
                            <div class="title">
                                <span class="fs1" aria-hidden="true" data-icon="&#xe14a;"></span> Customizar Veículo
                            </div>
                        </div>
                        <div class="widget-body">
                            <div id="wizard">
                                <ol>
                                    <li>Roda</li>
                                    <li>Cor</li>
                                    <li>Banco</li>
                                </ol>
                                <div>
                                    @Html.Partial("_partial/_CreateRoda", Model.Roda)
                         

                                    @*@Html.Partial("~/Views/Shared/_partial/_btn_submit_salvar.cshtml")*@

                                </div>

                                <div>
                                    @Html.Partial("_partial/_CreateCor", Model.Cor)
                                </div>

                                <div>
                                   @Html.Partial("_partial/_CreateBanco", Model.Banco)
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
}


